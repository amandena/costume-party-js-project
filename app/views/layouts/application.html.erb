<!DOCTYPE html>
<html>
  <head>
    <title>CostumePartyRailsProject</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="navbar">
      <% if !logged_in? %>
        <a href="#">Sign Up</a>
        <a href="#">Log In</a>
      <% else %>
        <a href="#">Log Out</a>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
