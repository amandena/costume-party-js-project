<%= form_for @party do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :time %>
  <%= f.text_field :time %><br>

  <%= f.label :date %>
  <%= f.text_field :date %><br>

  <%= f.label :user_id %>
  <%= f.collection_select :user_id, User.all, :id, :email %><br>

  <%= f.submit %>
<% end %>
